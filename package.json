{
    "name": "client",
    "version": "0.1.0",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build --prod",
        "lint": "vue-cli-service lint",
        "lint:fix": "vue-cli-service lint --fix",
        "test:e2e": "vue-cli-service test:e2e",
        "test:unit": "vue-cli-service test:unit",
        "encrypt:env": "gcloud kms encrypt --plaintext-file=.env --ciphertext-file=.env.enc --location=global --keyring=yaml-snippets --key=yaml-snippets-env"
    },
    "dependencies": {
        "algoliasearch": "^3.35.1",
        "core-js": "^3.2.1",
        "faker": "^4.1.0",
        "firebase": "^7.2.0",
        "firebaseui": "^4.2.0",
        "lodash": "^4.17.15",
        "prismjs": "^1.17.1",
        "register-service-worker": "^1.6.2",
        "spectre.css": "^0.5.8",
        "validate.js": "^0.13.1",
        "vue": "^2.6.10",
        "vue-clipboard2": "^0.3.1",
        "vue-router": "^3.1.3",
        "vuex": "^3.1.1"
    },
    "devDependencies": {
        "@types/algoliasearch": "^3.34.3",
        "@types/faker": "^4.1.6",
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.12",
        "@vue/cli-plugin-babel": "^3.12.0",
        "@vue/cli-plugin-e2e-cypress": "^3.12.0",
        "@vue/cli-plugin-pwa": "^3.12.0",
        "@vue/cli-plugin-typescript": "^3.12.0",
        "@vue/cli-plugin-unit-jest": "^3.12.0",
        "@vue/cli-service": "^3.12.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "babel-core": "7.0.0-bridge.0",
        "chance": "^1.1.3",
        "husky": "^3.0.8",
        "node-sass": "^4.12.0",
        "sass-loader": "^8.0.0",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.2",
        "ts-jest": "^24.1.0",
        "typescript": "^3.6.4",
        "vue-template-compiler": "^2.6.10"
    },
    "husky": {
        "hooks": {
            "pre-commit": "vue-cli-service lint --fix && git add .",
            "pre-push": "npm run test:unit"
        }
    }
}
